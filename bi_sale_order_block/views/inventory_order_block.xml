<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock_order_block_view_form" model="ir.ui.view">
        <field name="name">stock.order.block.view_form.inherit.sale</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="stock.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//div[@data-key='stock']" position="inside">
                <h2>Stock Move</h2>
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane">
                            <div class="content-group mt16">
                                <label for="stock_move" class="o_light_label"/>
                                <field name="stock_move" class="oe_inline"/>
                            </div>
                        </div>
                    </div>
            </xpath>
        </field>
    </record>
</odoo>
