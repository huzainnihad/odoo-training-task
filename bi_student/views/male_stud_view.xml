<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <data>
        <record id="action_stud_male_details" model="ir.actions.act_window">
            <field name="name">Male Students</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">bi.student</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{}</field>
            <field name="domain">[('stud_gender','=','male')]</field> 
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                Create your first male student</p>
            </field>
        </record>

    <menuitem
      id="menu_male_student"
      name="Male Student"
      action="action_stud_male_details"
      parent= "menu_student_master"
      sequence="4"/>

    </data>
</odoo>
